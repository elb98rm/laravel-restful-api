{
    "name": "floor9design/laravel-restful-api",
    "type": "library",
    "description": "A set of restful API classes that offer strict RESTful methods for laravel",
    "keywords": [
        "floor9design",
        "laravel",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/floor9design-ltd/laravel-restful-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Rick Morice",
            "email": "rick@floor9design.com",
            "homepage": "http://floor9design.com",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/inflector" : "^2.0",
        "ext-json": "*",
        "floor9design/json-api-formatter": "*",
        "illuminate/http" : "^7.0",
        "illuminate/support" : "^7.0",
        "illuminate/pagination" : "^7.0",
        "php" : ">=7.4",
        "symfony/yaml" : "^v5.0.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Floor9design\\LaravelRestfulApi\\": "src",
            "Floor9design\\LaravelRestfulApi\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
